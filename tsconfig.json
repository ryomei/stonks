{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "atoms": [
        "dist/atoms"
      ],
      "atoms/*": [
        "dist/atoms/*"
      ],
      "molecules": [
        "dist/molecules"
      ],
      "molecules/*": [
        "dist/molecules/*"
      ],
      "organisms": [
        "dist/organisms"
      ],
      "organisms/*": [
        "dist/organisms/*"
      ],
      "templates": [
        "dist/templates"
      ],
      "templates/*": [
        "dist/templates/*"
      ],
      "pages": [
        "dist/pages"
      ],
      "pages/*": [
        "dist/pages/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}